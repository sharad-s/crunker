{
  "name": "crunker",
  "version": "0.2.0",
  "description": "Simple way to merge or concatenate audio files with the Web Audio API.",
  "main": "dist/crunker.js",
  "directories": {
    "test": "test",
    "src": "src"
  },
  "scripts": {
    "build:test": "webpack --config webpack.test.config.js --mode development",
    "build:prod": "webpack --config webpack.config.js --mode production",
    "build:dev":"babel src/crunker.js -o dist/crunker.babel.js",
    "prepare": "npm run build:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackedgson/crunker.git"
  },
  "keywords": [
    "web-audio-api",
    "es6",
    "merge",
    "concatenate",
    "append",
    "export",
    "download"
  ],
  "author": "Jack Edgson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackedgson/crunker/issues"
  },
  "homepage": "https://github.com/jackedgson/crunker#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-minify": "^0.3.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "webpack": "^4.17.0",
    "webpack-cli": "^2.0.13"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "web-audio-api": "^0.2.2"
  }
}
